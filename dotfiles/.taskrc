urgency.user.tag.mq.coefficient=3
urgency.user.tag.sq.coefficient=1.5

uda.jira.type=string
uda.jira.label=Jira

uda.priority.values=iu,inu,niu,ninu,
urgency.uda.priority.iu.coefficient=6
urgency.uda.priority.inu.coefficient=3
urgency.uda.priority.niu.coefficient=2
urgency.uda.priority.ninu.coefficient=1

uda.reviewed.type=date
uda.reviewed.label=Reviewed

report._reviewed.description=Tasksh review report.  Adjust the filter to your needs.
report._reviewed.columns=uuid
report._reviewed.sort=reviewed+,modified+
report._reviewed.filter=( reviewed.none: or reviewed.before:now-6days ) and ( +PENDING or +WAITING )

# Above 1, these tend to mean "more structured equals more urgent." That's not helpful to me.
urgency.blocking.coefficient=0
urgency.tags.coefficient=0

report.next.filter=status:pending -WAITING -BLOCKED limit:page
